    {
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROMETHEUS}"
      },
      "description": "Total traffic rate in bytes per second",
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 10000000
              },
              {
                "color": "red",
                "value": 100000000
              }
            ]
          },
          "unit": "Bps"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 4,
        "x": 0,
        "y": 0
      },
      "options": {
        "orientation": "auto",
        "reduceOptions": {
          "calcs": []
        },
        "showThresholdLabels": false,
        "showThresholdMarkers": true
      },
      "repeatDirection": "h",
      "targets": [
        {
          "expr": "rate(goflow_bytes_all_total{instance=\"$instance\"}[$__rate_interval])",
          "instant": false,
          "legendFormat": "Traffic Rate",
          "range": true,
          "refId": "A"
